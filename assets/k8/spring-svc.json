{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "spring-rest",
    "namespace": "default",
    "labels": {
      "service": "spring-rest"
    }
  },
  "spec": {
    "ports": [
      {
        "protocol": "TCP",
        "port": 8080,
        "targetPort": "http"
      }
    ],
    "selector": {
      "app": "spring-rest"
    },
    "sessionAffinity": "None",
    "type": "NodePort"
  }
}
