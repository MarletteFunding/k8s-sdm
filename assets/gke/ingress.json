{
  "kind": "Ingress",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "atm-gke",
    "namespace": "default",
    "annotations": {
      "ingress.kubernetes.io/rewrite-target": "/"
    },
    "labels": {
      "ingress": "global"
    }
  },
  "spec": {
    "rules": [
      {
        "http": {
          "paths": [
            {
              "backend": {
                "serviceName": "hostname",
                "servicePort": 80
              },
              "path": "/health"
            }
          ]
        }
      }
    ]
  }
}
