{
  "apiVersion": "extensions/v1beta1",
  "kind": "Deployment",
  "metadata": {
    "name": "hostname",
    "namespace": "default"
  },
  "spec": {
    "replicas": 1,
    "revisionHistoryLimit": 3,
    "selector": {
      "matchLabels": {
        "app": "hostname"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "hostname"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "hostname",
            "image": "k8s.gcr.io/serve_hostname",
            "ports": [
              {
                "name": "http",
                "containerPort": 9376,
                "protocol": "TCP"
              }
            ]
          }
        ]
      }
    }
  }
}
